      "homepage": "https://laudis.tech"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "slim/slim": "^3.1",
    "monolog/monolog": "^1.17",
    "psr/container": "1.0.*",
    "psr/http-message": "1.0.*",
    "rakit/validation": "^1.1",
    "ext-json": "*",
    "vlucas/phpdotenv": "^3.3",
    "ext-intl": "*",
    "ext-pdo": "*",
    "nilportugues/sql-query-builder": "^1.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "8.0.*",
    "codedungeon/phpunit-result-printer": "^0.26.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Laudis\\Calculators\\Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Laudis\\Calculators\\": "src"
    },
    "files": [
      "src/helpers.php",
      "src/settings.php"
    ]
  },
  "config": {
    "process-timeout": 0
  }
}
